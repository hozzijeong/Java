package ex_array;

public class Repeat_Ex_array09_saveonly4 {
	public static void main(String[] args) {
		int[] arr = {44, 11, 29, 24, 76};
		int[] temp = null;

		int cnt = 0;
		for(int i=0; i<5; i++) {
			if(arr[i] %4 ==0) {
				cnt+=1;
			}
		}
		temp = new int[cnt];
		int k =0;
		for(int i=0; i<5; i++) {
			if(arr[i]%4 ==0) {
				temp[k] = arr[i];
				k+=1;
			}
		}
		
		
	}
}
